  "scripts": {
    "start": "tsnd --transpile-only --respawn --ignore-watch node_modules src/index.ts",
    "start:prod": "ts-node-dev --transpile-only --respawn --ignore-watch node_modules dist/index.js",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "migrations": "yarn sequelize db:migrate",
    "undo": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "config:swagger": "ts-node -r tsconfig-paths/register dist/documentation/swaggerDocGen.js",
    "config-dev:swagger": "ts-node -r tsconfig-paths/register src/documentation/swaggerDocGen.ts",
    "debug": "tsc --watch",
    "test": "jest"
  },
